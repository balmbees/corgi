{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "interface-name": [true, "never-prefix"],
        "trailing-comma": [false],
        "arrow-parens": false,
        "only-arrow-functions": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
